{
    executorImage: argoproj/argoexec:v2.3.0,
    containerRuntimeExecutor: docker,
    artifactRepository: {
        s3: {
            bucket: mlpipeline,
            keyPrefix: artifacts,
            endpoint: "minio-service.${namespace}:9000",
            insecure: true,
            accessKeySecret: {
                name: ${minio_secret},
                key: accesskey
            },
            secretKeySecret: {
                name: ${minio_secret},
                key: secretkey
            }
        }
    }
}